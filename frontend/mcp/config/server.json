{
  "mcpServers": {
    "trabahanap-chakra": {
      "command": "node",
      "args": ["mcp/servers/chakra-server.js"],
      "cwd": "${workspaceFolder}",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "trabahanap-react": {
      "command": "node",
      "args": ["mcp/servers/react-server.js"],
      "cwd": "${workspaceFolder}",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "trabahanap-api": {
      "command": "node",
      "args": ["mcp/servers/api-server.js"],
      "cwd": "${workspaceFolder}",
      "env": {
        "NODE_ENV": "development"
      }
    }
  }
}